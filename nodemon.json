{
  "watch": ["./"],
  "ext": ".ts,.js",
  "ignore": [
    "build",
    "node_modules"
  ],
  "execBackup": "babel-node src --out-dir lib --extensions \".ts,.js\" --ignore node_modules --source-maps inline --inspect-brk",
  "exec": "babel-node jasmine.node.config.js --extensions \".ts,.js\" --ignore node_modules --source-maps inline --inspect-brk"
}

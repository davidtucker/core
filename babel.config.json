{
  "presets": [
    "@babel/preset-typescript",
    ["@babel/preset-env", {
      "modules": false
    }]
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/plugin-proposal-private-methods",
    "@babel/plugin-transform-runtime",
    ["@babel/plugin-proposal-decorators", {
      "legacy": true
    }]
  ],
  "ignore": [
    "node_modules"
  ]
}

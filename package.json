{
  "name": "@pregular/core",
  "version": "0.0.0",
  "description": "Ai based, Fast, SSR, Lightweight Angular alternative with the same modern API thats compile directly to WebComponents",
  "keywords": [
    "angular",
    "artificial",
    "component",
    "fast",
    "intelligence",
    "pregular",
    "small",
    "tree-shaking",
    "typescript"
  ],
  "homepage": "https://github.com/pregular/core#readme",
  "bugs": {
    "url": "https://github.com/pregular/core/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pregular/core.git"
  },
  "license": "MIT",
  "author": "Serkan Sipahi <serkan.sipahi@yahoo.de>",
  "main": "index.js",
  "scripts": {
    "build:dev": "echo 'warning: 'build:dev' not implemented yet'",
    "build:prod": "echo 'warning: build:prod' not implemented yet'",
    "clean": "find ./packages -type f \\( -iname '*.cjs' -o -iname '*.js' -o -iname '*.d.ts' \\) -delete",
    "compile": "npm run clean && npm run compile:cjs:all && npm run compile:mjs:all && npm run compile:dec:all",
    "compile:cjs:all": "babel ./packages --out-dir ./packages -x .ts --out-file-extension .cjs --plugins @babel/plugin-transform-modules-commonjs --source-maps inline",
    "compile:cjs:single": "babel ${npm_config_src_file} --out-file ${npm_config_out_file} --plugins @babel/plugin-transform-modules-commonjs --source-maps inline",
    "compile:dec:all": "tsc ./packages/pkg-*/src/*.ts --emitDeclarationOnly --declaration --target ES5 --allowJs false",
    "compile:dec:single": "tsc ${npm_config_src_file} --emitDeclarationOnly --declaration --target ES5 --allowJs false",
    "compile:mjs:all": "babel ./packages --out-dir ./packages -x .ts --out-file-extension .js --source-maps inline",
    "compile:mjs:single": "babel ${npm_config_src_file} --out-file ${npm_config_out_file} --source-maps inline",
    "compile:watch": "npm run clean && node scripts/babel.compiler.js",
    "eslint": "eslint ./ --ext .js,.ts --ignore-pattern '**/node_modules/**' --cache",
    "eslint:fix": "npm run eslint -- --fix",
    "lint": "npm run eslint && npm run tslint",
    "lint-staged": "lint-staged --no-stash",
    "lint:fix": "npm run eslint:fix && npm run tslint",
    "lint:watch": "npm run eslint:watch && npm run tslint -- -w",
    "ncu": "ncu",
    "ncu:update": "ncu -u",
    "prettier:write": "prettier --write ./ --config .prettierrc",
    "sort-pkg": "sort-package-json",
    "test": "npm run test:all:watch",
    "test:all": "concurrently \"npm run test:node\" \"npm run test:browser\"",
    "test:all:headless": "concurrently \"npm run test:node\" \"npm run test:browser:headless\"",
    "test:all:headless:watch": "concurrently \"npm run test:node:watch\" \"npm run test:browser:headless:watch\"",
    "test:all:watch": "concurrently \"npm run test:node:watch\" \"npm run test:browser:watch\"",
    "test:browser": "karma start scripts/karma.conf.js --auto-watch=false --single-run=true",
    "test:browser:coverage": "karma start scripts/karma.conf.js --coverage",
    "test:browser:headless": "karma start scripts/karma.conf.js --browsers ChromeHeadless,FirefoxHeadless --auto-watch=false --single-run=true",
    "test:browser:headless:watch": "karma start scripts/karma.conf.js --browsers ChromeHeadless,FirefoxHeadless --auto-watch=true --single-run=false",
    "test:browser:watch": "karma start scripts/karma.conf.js --auto-watch=true --single-run=false",
    "test:headless": "concurrently \"npm run test:node\" \"npm run test:browser:headless\"",
    "test:headless:watch": "concurrently \"npm run test:node:watch\" \"npm run test:browser:headless:watch\"",
    "test:node": "node scripts/jasmine.node.config.js",
    "test:node:coverage": "echo 'test:node:coverage not implemented yet'",
    "test:node:watch": "nodemon --exec node scripts/jasmine.node.config.js --extensions \".cjs\" --ignore node_modules",
    "test:node:watch:inspect": "nodemon --exec babel-node scripts/jasmine.node.config.js --extensions \".cjs\" --ignore node_modules --inspect-brk",
    "tslint": "tsc --noEmit --allowJs --target ES5 --declaration false --declarationMap false"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --no-stash"
    }
  },
  "lint-staged": {
    "packages/**/*!(.d).ts": [
      "npm run tslint",
      "npm run eslint:fix",
      "npm run compile",
      "npm run test:node",
      "npm run test:browser:headless"
    ]
  },
  "browserslist": [
    "last 1 version",
    "not IE 11",
    "not IE_Mob 11"
  ],
  "dependencies": {
    "core-js": "3.6.5",
    "document-register-element": "1.14.3"
  },
  "devDependencies": {
    "@angular/compiler": "9.1.9",
    "@babel/cli": "7.10.1",
    "@babel/core": "7.10.2",
    "@babel/node": "7.10.1",
    "@babel/plugin-proposal-class-properties": "7.10.1",
    "@babel/plugin-proposal-decorators": "7.10.1",
    "@babel/plugin-proposal-private-methods": "7.10.1",
    "@babel/plugin-transform-runtime": "7.10.1",
    "@babel/preset-env": "7.10.2",
    "@babel/preset-typescript": "7.10.1",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@open-wc/karma-esm": "2.16.12",
    "@types/jasmine": "3.5.10",
    "@types/node": "14.0.11",
    "@types/shelljs": "0.8.8",
    "@typescript-eslint/eslint-plugin": "3.1.0",
    "colors": "1.4.0",
    "concurrently": "5.2.0",
    "eslint": "7.1.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-config-airbnb-typescript": "8.0.2",
    "eslint-config-airbnb-typescript-prettier": "2.1.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-unicorn": "20.1.0",
    "eslint-watch": "7.0.0",
    "eslint_d": "8.1.1",
    "get-value": "3.0.1",
    "husky": "4.2.5",
    "jasmine": "3.5.0",
    "jasmine-console-reporter": "3.1.0",
    "jasmine-core": "3.5.0",
    "jasmine-node": "3.0.0",
    "karma": "5.0.9",
    "karma-chrome-launcher": "3.1.0",
    "karma-cli": "2.0.0",
    "karma-edge-launcher": "0.4.2",
    "karma-firefox-launcher": "1.3.0",
    "karma-jasmine": "3.3.1",
    "karma-opera-launcher": "1.0.0",
    "karma-safari-launcher": "1.0.0",
    "lerna": "3.22.0",
    "lint-staged": "10.2.9",
    "minimatch": "3.0.4",
    "node-watch": "0.6.4",
    "nodemon": "2.0.4",
    "npm-check-updates": "6.0.1",
    "prettier": "2.0.5",
    "shelljs": "0.8.4",
    "sort-package-json": "1.44.0",
    "tslib": "2.0.0",
    "typescript": "3.9.5"
  }
}

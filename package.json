{
  "name": "@pregular/core",
  "version": "0.0.0",
  "description": "Ai based, Fast, SSR, Lightweight Angular alternative with the same modern API thats compile directly to WebComponents",
  "keywords": [
    "angular",
    "artificial",
    "component",
    "fast",
    "intelligence",
    "pregular",
    "small",
    "tree-shaking",
    "typescript"
  ],
  "homepage": "https://github.com/pregular/core#readme",
  "bugs": {
    "url": "https://github.com/pregular/core/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pregular/core.git"
  },
  "license": "MIT",
  "author": "Serkan Sipahi <serkan.sipahi@yahoo.de>",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:clean && npm run build:types && npm run build:ts",
    "build:clean": "rm -rf ./lib && rm -rf ./build",
    "build:ts": "nodemon",
    "build:types": "tsc --emitDeclarationOnly",
    "lerna:init": "lerna init",
    "test-compile": "babel src --out-dir lib --extensions \".ts,.js\"",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "test": "karma start --coverage",
    "test:watch": "karma start --coverage --auto-watch=true --single-run=false"
  },
  "browserslist": [
    "last 1 version",
    "not IE 11",
    "not IE_Mob 11"
  ],
  "dependencies": {
    "core-js": "3.6.4",
    "document-register-element": "1.14.3"
  },
  "devDependencies": {
    "@angular/compiler": "9.0.6",
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.7",
    "@babel/node": "7.8.7",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-private-methods": "7.8.3",
    "@babel/preset-env": "7.8.7",
    "@babel/preset-typescript": "7.8.3",
    "@open-wc/karma-esm": "2.13.19",
    "@types/jasmine": "3.5.9",
    "@types/node": "13.9.1",
    "jasmine": "3.5.0",
    "jasmine-console-reporter": "3.1.0",
    "jasmine-core": "3.5.0",
    "karma": "4.4.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-cli": "2.0.0",
    "karma-edge-launcher": "0.4.2",
    "karma-firefox-launcher": "1.3.0",
    "karma-jasmine": "3.1.1",
    "karma-opera-launcher": "1.0.0",
    "karma-safari-launcher": "1.0.0",
    "lerna": "3.20.2",
    "nodemon": "2.0.2",
    "tslib": "1.11.1",
    "typescript": "3.7.4"
  }
}

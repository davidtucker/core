{
  "name": "@pregular/core",
  "version": "0.0.0",
  "description": "Ai based, Fast, SSR, Lightweight Angular alternative with the same modern API thats compile directly to WebComponents",
  "keywords": [
    "angular",
    "artificial",
    "component",
    "fast",
    "intelligence",
    "pregular",
    "small",
    "tree-shaking",
    "typescript"
  ],
  "homepage": "https://github.com/pregular/core#readme",
  "bugs": {
    "url": "https://github.com/pregular/core/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pregular/core.git"
  },
  "license": "MIT",
  "author": "Serkan Sipahi <serkan.sipahi@yahoo.de>",
  "main": "index.js",
  "scripts": {
    "build:dev": "npm run clean && node babel.compiler.js",
    "build:prod": "npm run clean && echo 'build:prod not implemented yet'",
    "clean": "find ./packages -type f \\( -iname \\*.cjs -o -iname \\*.js -o -iname \\*.d.ts \\) -delete",
    "compile:cjs:all": "babel ./packages --out-dir ./packages -x .ts --out-file-extension .cjs --plugins @babel/plugin-transform-modules-commonjs --source-maps inline",
    "compile:cjs:single": "babel ${npm_config_src_file} --out-file ${npm_config_out_file} --plugins @babel/plugin-transform-modules-commonjs --source-maps inline",
    "compile:dec:all": "tsc ./packages/pg-*/src/*.ts --declaration --target ES5",
    "compile:dec:single": "tsc ${npm_config_src_file} --emitDeclarationOnly --declaration",
    "compile:mjs:all": "babel ./packages --out-dir ./packages -x .ts --out-file-extension .js --source-maps inline",
    "compile:mjs:single": "babel ${npm_config_src_file} --out-file ${npm_config_out_file} --source-maps inline",
    "lint": "npm run eslint && npm run tslint",
    "lint:fix": "npm run eslint -- --fix && npm run tslint",
    "lint:watch": "npm run eslint:watch && npm run tslint -- -w",
    "test:all": "concurrently \"npm run test:node\" \"npm run test:browser\"",
    "test:all:watch": "concurrently \"npm run test:node:watch\" \"npm run test:browser:watch\"",
    "test:browser": "karma start --coverage --auto-watch=false --single-run=true",
    "test:browser:coverage": "karma start --coverage",
    "test:browser:watch": "karma start --coverage --auto-watch=true --single-run=false",
    "test:node": "babel-node jasmine.node.config.js --ignore node_modules",
    "test:node:coverage": "echo 'test:node:coverage not implemented yet'",
    "test:node:watch": "nodemon --exec babel-node jasmine.node.config.js --extensions \".js\" --ignore node_modules",
    "test:node:watch:inspect": "nodemon --exec babel-node jasmine.node.config.js --extensions \".js\" --ignore node_modules--inspect-brk",
    "tslint": "tsc --noEmit",
    "eslint": "eslint ./ --ext .js,.ts --ignore-pattern '**/*.d.ts'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "git add"
    ]
  },
  "browserslist": [
    "last 1 version",
    "not IE 11",
    "not IE_Mob 11"
  ],
  "dependencies": {
    "core-js": "3.6.4",
    "document-register-element": "1.14.3"
  },
  "devDependencies": {
    "@angular/compiler": "9.1.0",
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/node": "7.8.7",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-private-methods": "7.8.3",
    "@babel/preset-env": "7.9.0",
    "@babel/preset-typescript": "7.9.0",
    "@open-wc/karma-esm": "2.13.21",
    "@types/jasmine": "3.5.10",
    "@types/node": "13.9.5",
    "@types/shelljs": "0.8.7",
    "@typescript-eslint/eslint-plugin": "2.26.0",
    "concurrently": "5.1.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-typescript": "7.2.0",
    "eslint-config-airbnb-typescript-prettier": "2.1.1",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-watch": "6.0.1",
    "husky": "4.2.3",
    "jasmine": "3.5.0",
    "jasmine-console-reporter": "3.1.0",
    "jasmine-core": "3.5.0",
    "jasmine-node": "3.0.0",
    "karma": "4.4.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-cli": "2.0.0",
    "karma-edge-launcher": "0.4.2",
    "karma-firefox-launcher": "1.3.0",
    "karma-jasmine": "3.1.1",
    "karma-opera-launcher": "1.0.0",
    "karma-safari-launcher": "1.0.0",
    "lerna": "3.20.2",
    "node-watch": "0.6.3",
    "nodemon": "2.0.2",
    "prettier": "2.0.2",
    "shelljs": "0.8.3",
    "tslib": "1.11.1",
    "typescript": "3.8.3"
  }
}

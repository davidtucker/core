{
  "name": "@pregular/core",
  "version": "0.0.0",
  "description": "Ai based, Fast, SSR, Lightweight Angular alternative with the same modern API thats compile directly to WebComponents",
  "keywords": [
    "angular",
    "artificial",
    "component",
    "fast",
    "intelligence",
    "pregular",
    "small",
    "tree-shaking",
    "typescript"
  ],
  "homepage": "https://github.com/pregular/core#readme",
  "bugs": {
    "url": "https://github.com/pregular/core/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pregular/core.git"
  },
  "license": "MIT",
  "author": "Serkan Sipahi <serkan.sipahi@yahoo.de>",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:clean && npm run build:types && npm run build:ts",
    "build:new": "node babel.compiler.js",
    "build:types": "tsc --emitDeclarationOnly",
    "clean": "find ./packages -type f \\( -iname \\*.cjs -o -iname \\*.js \\) -delete",
    "test:all:watch": "concurrently \"npm run build:new\" \"npm run test:node:watch\" \"npm run test:browser:watch\"",
    "test:browser:coverage": "karma start --coverage",
    "test:browser:watch": "karma start --coverage --auto-watch=true --single-run=false",
    "test:node": "babel-node jasmine.node.config.ts --ignore node_modules",
    "test:node:watch": "nodemon --exec babel-node jasmine.node.config.ts --extensions \".ts,.js\" --ignore node_modules --source-maps inline",
    "test:node:watch:inspect": "nodemon --exec babel-node jasmine.node.config.ts --extensions \".ts,.js\" --ignore node_modules --source-maps inline --inspect-brk",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "watch": "watch ./packages"
  },
  "browserslist": [
    "last 1 version",
    "not IE 11",
    "not IE_Mob 11"
  ],
  "dependencies": {
    "core-js": "3.6.4",
    "document-register-element": "1.14.3"
  },
  "devDependencies": {
    "@angular/compiler": "9.1.0",
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/node": "7.8.7",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-private-methods": "7.8.3",
    "@babel/preset-env": "7.9.0",
    "@babel/preset-typescript": "7.9.0",
    "@open-wc/karma-esm": "2.13.21",
    "@types/jasmine": "3.5.10",
    "@types/node": "13.9.5",
    "@types/shelljs": "0.8.7",
    "concurrently": "5.1.0",
    "jasmine": "3.5.0",
    "jasmine-console-reporter": "3.1.0",
    "jasmine-core": "3.5.0",
    "jasmine-node": "3.0.0",
    "karma": "4.4.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-cli": "2.0.0",
    "karma-edge-launcher": "0.4.2",
    "karma-firefox-launcher": "1.3.0",
    "karma-jasmine": "3.1.1",
    "karma-opera-launcher": "1.0.0",
    "karma-safari-launcher": "1.0.0",
    "lerna": "3.20.2",
    "node-watch": "0.6.3",
    "nodemon": "2.0.2",
    "shelljs": "0.8.3",
    "tslib": "1.11.1",
    "typescript": "3.8.3"
  }
}
